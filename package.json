{
  "name": "@edwinspire/libopenfusionapigui",
  "version": "1.1.10",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwinspire/libOpenFusionAPIGUI.git"
  },
  "keywords": [
    "svelte",
    "javascript",
    "component",
    "OpenFusionAPI",
    "edwinspire",
    "api"
  ],
  "author": "edwinspire",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/edwinspire/libOpenFusionAPIGUI.git/issues"
  },
  "homepage": "https://github.com/edwinspire/libOpenFusionAPIGUI.git#readme",
  "description": "GUI for OpenFusionAPI",
  "scripts": {
    "dev": "vite dev",
    "build": "node ./set_version.js && vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.20.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "8.56.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "publint": "^0.1.9",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.11"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@edwinspire/svelte-components": "https://github.com/edwinspire/svelte-components.git",
    "@edwinspire/universal-fetch": "https://github.com/edwinspire/universal-fetch.git",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "bulma": "^1.0.4",
    "echarts": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^2.0.2"
  }
}
